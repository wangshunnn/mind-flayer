{
  "name": "sidecar",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "AI SDK sidecar for Mind Flayer",
  "main": "dist/index.js",
  "scripts": {
    "clean:binaries": "rimraf ../src-tauri/binaries/*",
    "dev": "npm run clean:binaries && NODE_ENV=development tsup && bash scripts/rename-binaries.sh",
    "build": "npm run clean:binaries && NODE_ENV=production tsup && pkg dist/index.js -t node24-macos-arm64 -o ../src-tauri/binaries/mind-flayer-sidecar-aarch64-apple-darwin",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.23",
    "@ai-sdk/openai": "^3.0.18",
    "@hono/node-server": "^1.13.7",
    "ai": "^6.0.49",
    "hono": "^4.7.11",
    "parallel-web": "^0.2.4",
    "undici": "^7.19.0",
    "vercel-minimax-ai-provider": "^0.0.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@vitest/ui": "^4.0.18",
    "@yao-pkg/pkg": "^6.12.0",
    "dotenv": "^17.2.3",
    "msw": "^2.12.7",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.1",
    "typescript": "~5.9.3",
    "vitest": "^4.0.18"
  }
}
